{
	"name":"YT Speed Buttons",
	"description":"Displays Speed Buttons under video player and saves clicks.",
	"version":"1.0.0",
	"manifest_version":2,
	"background": {
		"scripts": ["assets/jquery-3.1.1.min.js", "SpeedButtons.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": ["https://www.youtube.com/watch?v=*"],
			"css": ["assets/styles.css"],
			"js": ["assets/jquery-3.1.1.min.js", "SpeedButtons.js"]
		}
	]
}
